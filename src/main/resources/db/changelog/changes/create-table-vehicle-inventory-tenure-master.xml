<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
	xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
	xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd
    http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">

	<changeSet author="piyush"
		id="create-table-vehicle-inventory-tenure-master">

		<createTable tableName="vehicle_inventory_tenure_master"
			schemaName="public">
			<column name="vehicle_inventory_id" type="VARCHAR" />
			<column name="tenure_master_tenure_duration" type="VARCHAR" />
		</createTable>
		<addForeignKeyConstraint
			baseColumnNames="vehicle_inventory_id"
			baseTableName="vehicle_inventory_tenure_master"
			baseTableSchemaName="public" constraintName="vehicle_inventory_fkid"
			referencedColumnNames="id" referencedTableName="vehicle_inventory"
			referencedTableSchemaName="public" />
		<addForeignKeyConstraint
			baseColumnNames="tenure_master_tenure_duration"
			baseTableName="vehicle_inventory_tenure_master"
			baseTableSchemaName="public" constraintName="tenure_master_fkid"
			referencedColumnNames="tenure_duration"
			referencedTableName="tenure_price_master"
			referencedTableSchemaName="public" />
		<addPrimaryKey
			tableName="vehicle_inventory_tenure_master"
			columnNames="vehicle_inventory_id, tenure_master_tenure_duration" />
	</changeSet>
</databaseChangeLog>